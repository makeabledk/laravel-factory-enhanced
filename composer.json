{
    "name": "makeabledk/laravel-factory-enhanced",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelFactory\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/laravel": "^9.0|^10.0",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelFactory\\Tests\\": "tests/",
            "Makeable\\LaravelFactory\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelFactory\\FactoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
