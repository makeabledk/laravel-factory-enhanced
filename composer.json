{
    "name": "makeabledk/laravel-factory-enhanced",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelFactory\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^7.3|8.*",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "fzaninotto/faker": "^1.4"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30",
        "doctrine/dbal": "^3.0",
        "mockery/mockery": "^1.4",
        "laravel/laravel": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.5.3|^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelFactory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelFactory\\FactoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
